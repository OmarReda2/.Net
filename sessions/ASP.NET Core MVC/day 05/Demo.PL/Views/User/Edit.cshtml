@model ApplicationUser


@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>
<br />

<form asp-action="Edit">
    <input type="hidden" asp-for="Id"/>


    <div class="form-group">
        <label asp-for="UserName" class="col-form-label"></label>
        <input asp-for="UserName" class="form-control" />
        <span class="text-danger" asp-validation-for="UserName"></span>
    </div>

    <div class="form-group">
        <label asp-for="Email" class="col-form-label"></label>
        <input asp-for="Email" class="form-control" />
        <span class="text-danger" asp-validation-for="Email"></span>
    </div>

  
    <br />
    <div class="demo-checkbox">
        <input type="checkbox" asp-for="IsAgree" />
        <label asp-for="IsAgree"></label>
        <span asp-validation-for="IsAgree" class="text-danger"></span>
    </div>
    <br />


    <input type="submit" class="btn btn-primary" value="Update" />
    <a asp-action="Index" class="btn btn-dark">Back Tio List</a>
</form>

@section Validation {
    <partial name="_ValidationScriptsPartial" />
}



