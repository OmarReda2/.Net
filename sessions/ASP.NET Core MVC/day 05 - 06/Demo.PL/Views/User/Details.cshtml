@model ApplicationUser


@{
    ViewData["Title"] = "Details";
}

<h1>User Details</h1>
<br />

<div>
    <div class="form-group">
        <label asp-for="UserName" class="col-form-label"></label>
        <input asp-for="UserName" class="form-control" readonly/>
    </div>

    <div class="form-group">
        <label asp-for="Email" class="col-form-label"></label>
        <input asp-for="Email" class="form-control" readonly/>
    </div>

    <br />
    <div class="demo-checkbox">
        <input type="checkbox" asp-for="IsAgree" readonly/>
        <label asp-for="IsAgree"></label>
    </div>
    <br />

    <a asp-action="Edit" class="btn btn-success">Edit</a>
</div>